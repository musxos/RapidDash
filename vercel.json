{
    "version": 1,
    "env": {
        "MONGO_URI": "@mongo_uri",
        "INITIAL_PASSWORD": "@initial",
        "CDN_PATH":"@cdn"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}