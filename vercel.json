{
    "version": 2,
    "env": {
        "MONGO_URI": "mongodb://127.0.0.1:27017/rapid_chain",
        "INITIAL_PASSWORD": "123456",
        "CDN_PATH":""
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}